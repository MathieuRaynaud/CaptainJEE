<?xml version="1.0" encoding="UTF-8"?>
<web-app
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
        version="3.0">



    <servlet>
        <servlet-name>MyServlet2</servlet-name>
        <servlet-class>com.sdzee.servlets.Servlet2</servlet-class>

        <description>deuxieme essai</description>

        <init-param>
            <param-name>LaTeamJEE</param-name>
            <param-value>Coyote</param-value>
        </init-param>

        <load-on-startup>1</load-on-startup>

    </servlet>

    <servlet-mapping>
        <servlet-name>MyServlet2</servlet-name>
        <url-pattern>/servlet2</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Servlet_etat_des_charges</servlet-name>
        <servlet-class>com.sdzee.servlets.Servlet_etat_des_charges</servlet-class>

        <description>Etat des charges</description>

        <init-param>
            <param-name>LaTeamJEE</param-name>
            <param-value>Coyote</param-value>
        </init-param>

        <load-on-startup>1</load-on-startup>

    </servlet>

    <servlet-mapping>
        <servlet-name>Servlet_etat_des_charges</servlet-name>
        <url-pattern>/etat_des_charges</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Servlet_login</servlet-name>
        <servlet-class>com.sdzee.servlets.Servlet_login</servlet-class>

        <description>Etat des charges</description>

        <init-param>
            <param-name>LaTeamJEE</param-name>
            <param-value>Coyote</param-value>
        </init-param>

        <load-on-startup>1</load-on-startup>

    </servlet>

    <servlet-mapping>
        <servlet-name>Servlet_login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

</web-app>